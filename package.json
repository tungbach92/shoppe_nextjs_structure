  "dependencies": {
    "axios": "1.3.2",
    "axios-hooks": "^4.0.0",
    "daisyui": "2.50.0",
    "firebase": "9.17.1",
    "jotai": "2.0.0",
    "next": "13.1.6",
    "proxy-memoize": "2.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-firebase-hooks": "^5.1.1",
    "store": "^2.0.12",
    "valtio": "1.10.1"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.18.6",
    "@next/bundle-analyzer": "^13.1.6",
    "@types/node": "18.13.0",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/store": "^2.0.2",
    "autoprefixer": "10.4.13",
    "env-cmd": "^10.1.0",
    "eslint": "8.33.0",
    "eslint-config-next": "13.1.6",
    "next-transpile-modules": "10.0.0",
    "postcss": "8.4.21",
    "tailwindcss": "3.2.4",
    "typescript": "4.9.5"
